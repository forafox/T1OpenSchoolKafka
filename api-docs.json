 API"],"summary":"Get film by genre","description":"Get a film by its genre.","operationId":"getFilmByGenre","parameters":[{"name":"genre","in":"path","description":"Genre of the film to be obtained","required":true,"schema":{"type":"string"}}],"responses":{"200":{"description":"Successful operation","content":{"*/*":{"schema":{"$ref":"#/components/schemas/FilmDTO"}}}}}}},"/film/country/{country}":{"get":{"tags":["Film API"],"summary":"Get film by country","description":"Get a film by its country.","operationId":"getFilmByCountry","parameters":[{"name":"country","in":"path","description":"Country of the film to be obtained","required":true,"schema":{"type":"string"}}],"responses":{"200":{"description":"Successful operation","content":{"*/*":{"schema":{"$ref":"#/components/schemas/FilmDTO"}}}}}}}},"components":{"schemas":{"FilmDTO":{"type":"object","properties":{"id":{"type":"integer","description":"Unique identifier of the film","format":"int64","readOnly":true,"example":1},"titleName":{"type":"string","description":"Title name of the film","example":"Catch Me If You Can"},"genre":{"type":"string","description":"Genre of the film","example":"Comedy"},"country":{"type":"string","description":"Country of the film","example":"United States"},"producer":{"type":"string","description":"Producer of the film","example":"Steven Spielberg"}},"description":"DTO for Film entity"},"MethodDataDTO":{"type":"object","properties":{"executeDate":{"type":"string","description":"Execution date of the method","format":"date-time","example":"2024-05-04T23:29:18.525Z"},"methodName":{"type":"string","description":"Name of the method","example":"FilmServiceImpl.create(..)"},"executeNanoTime":{"type":"integer","description":"Execution time of the method (nano)","format":"int64","example":49},"executeMilliTime":{"type":"integer","description":"Execution time of the method (ms)","format":"int64","example":49},"annotationType":{"type":"string","description":"Annotation type of the method","exa